
pipeline {
  stages {
    stage("Build") {
       steps {
          // Just print a Hello, Pipeline to the console
          echo "Hello, Pipeline!"
           bat "\"${msbuildHome}\\MSBuild.exe\" /t:Rebuild"
            
             echo "building , Pipeline!"
             
       }
   }
   // And next stages if you want to define further...
 } // End of stages
} // End of pipeline
